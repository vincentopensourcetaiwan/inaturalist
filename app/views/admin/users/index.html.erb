<div class="row">
  <div class="col-xs-3">
  </div>
  <div class="col-xs-6"></div>
  <div class="col-xs-3">
  </div>
</div>
<%= paginate @users, theme: 'twitter-bootstrap-4', pagination_class: "pagination-sm", nav_class: "d-inline-b" %>

<table class="table table-striped table-bordered table-hover">
  <thead>
  <tr>
    <th class="text-center">id</th>
    <th class="text-center">email</th>
    <th class="text-center">name</th>
    <th class="text-center">nickname</th>
    <th class="text-center">roles</th>
    <th class="text-center">inaturalist_id</th>
    <th class="text-center">inaturalist_icon_url</th>
    <th class="text-center">inaturalist_login</th>
    <th class="text-center">operations</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td class="text-center"><%= user.id %></td>
      <td class="text-center"><%= user.email %></td>
      <td class="text-center"><%= user.name %></td>
      <td class="text-center"><%= user.nickname %></td>
      <td class="text-center"></td>
      <td class="text-center"><%= user.inaturalist_id %></td>
      <td class="text-center"><%= image_tag user.inaturalist_icon_url if user.inaturalist_icon_url.present? %></td>
      <td class="text-center"><%= user.inaturalist_login %></td>
      <td></td>
    </tr>
  <% end %>
  </tbody>
</table>
<%= paginate @users, theme: 'twitter-bootstrap-4', pagination_class: "pagination-sm", nav_class: "d-inline-b" %>
